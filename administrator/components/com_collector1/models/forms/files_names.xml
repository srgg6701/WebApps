                    readonly="true" class="readonly"
                    description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="files_names" type="editor" buttons="true"
                class="inputbox"
                filter="safehtml"
                 required="true" 
                
                
                
            label="COM_COLLECTOR1_FORM_LBL_FILES_NAMES_FILES_NAMES"
            description="COM_COLLECTOR1_FORM_DESC_FILES_NAMES_FILES_NAMES"  /> 

       <field name="identifier" type="text" size="40" class="inputbox"
                
            label="COM_COLLECTOR1_FORM_LBL_FILES_NAMES_IDENTIFIER"
            description="COM_COLLECTOR1_FORM_DESC_FILES_NAMES_IDENTIFIER" 
                
                
                 required="true" 
                filter="safehtml" /> 

         <field
                name="state"
                type="list"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1">
